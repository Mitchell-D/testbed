#SBATCH -e /rhome/mdodson/testbed/slurm/out/slurm_extract_tgframes_2000-2008.err # STDERR

cd /rhome/mdodson/testbed

#Run code
set runcmd = /nas/rhome/mdodson/.micromamba/envs/learn3/bin/python
