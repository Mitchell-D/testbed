	note = {Publisher: American Meteorological Society
Section: Earth Interactions},
	keywords = {Soil moisture, Hydroclimatology, Hydrologic budget, Land/atmosphere interactions, Unsaturated zone},
	pages = {1--26},
	file = {Full Text PDF:/home/krttd/tools/zotero/storage/A264C6UR/Miller and White - 1998 - A Conterminous United States Multilayer Soil Chara.pdf:application/pdf},
