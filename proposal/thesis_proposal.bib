	pages = {107--125},
	file = {ScienceDirect Snapshot:/home/krttd/tools/zotero/storage/ZAEGB8YA/S0022169414001358.html:text/html;Xia et al. - 2014 - Evaluation of multi-model simulated soil moisture .pdf:/home/krttd/tools/zotero/storage/XHL48WNG/Xia et al. - 2014 - Evaluation of multi-model simulated soil moisture .pdf:application/pdf;Xia et al. - 2014 - Evaluation of multi-model simulated soil moisture .pdf:/home/krttd/tools/zotero/storage/QFAURDSE/Xia et al. - 2014 - Evaluation of multi-model simulated soil moisture .pdf:application/pdf},
