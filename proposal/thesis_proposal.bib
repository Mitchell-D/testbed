	urldate = {2024-05-22},
	journal = {Journal of Applied Meteorology and Climatology},
	author = {Yang, Fanglin and Mitchell, Kenneth and Hou, Yu-Tai and Dai, Yongjiu and Zeng, Xubin and Wang, Zhuo and Liang, Xin-Zhong},
