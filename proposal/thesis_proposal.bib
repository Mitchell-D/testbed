	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/94JD00483},
	pages = {14415--14428},
	file = {Liang et al. - 1994 - A simple hydrologically based model of land surfac.pdf:/home/krttd/tools/zotero/storage/A3FGJZCD/Liang et al. - 1994 - A simple hydrologically based model of land surfac.pdf:application/pdf;Snapshot:/home/krttd/tools/zotero/storage/RGGV88M4/94JD00483.html:text/html},
